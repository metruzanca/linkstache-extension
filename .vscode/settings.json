{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll.eslint": true,
  },
  "javascript.preferences.quoteStyle": "single",
  "debug.internalConsoleOptions": "neverOpen",
}